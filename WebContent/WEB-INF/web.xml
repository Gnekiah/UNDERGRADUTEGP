<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>subsys_interactive</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>AdminAddTeacherServlet</servlet-name>
		<servlet-class>interactive.AdminAddTeacherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminAddTeacherServlet</servlet-name>
		<url-pattern>/adminaddteacher</url-pattern>
	</servlet-mapping>
	
	<servlet>
        <servlet-name>AdminDelTeacherServlet</servlet-name>
        <servlet-class>interactive.AdminDelTeacherServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminDelTeacherServlet</servlet-name>
        <url-pattern>/admindelteacher</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AdminDelStudentServlet</servlet-name>
        <servlet-class>interactive.AdminDelStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminDelStudentServlet</servlet-name>
        <url-pattern>/admindelstudent</url-pattern>
    </servlet-mapping>
	
	<servlet>
        <servlet-name>AddStudentServlet</servlet-name>
        <servlet-class>interactive.AddStudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddStudentServlet</servlet-name>
        <url-pattern>/addstudent</url-pattern>
    </servlet-mapping>
	
	<servlet>
		<servlet-name>StudentServlet</servlet-name>
		<servlet-class>interactive.StudentServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StudentServlet</servlet-name>
		<url-pattern>/student</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>TeacherServlet</servlet-name>
		<servlet-class>interactive.TeacherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TeacherServlet</servlet-name>
		<url-pattern>/teacher</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>interactive.LoginServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
        <servlet-name>ClassServlet</servlet-name>
        <servlet-class>interactive.ClassServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClassServlet</servlet-name>
        <url-pattern>/class</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>interactive.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>WorkServlet</servlet-name>
        <servlet-class>interactive.WorkServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WorkServlet</servlet-name>
        <url-pattern>/work</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ClassViewServlet</servlet-name>
        <servlet-class>interactive.ClassViewServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClassViewServlet</servlet-name>
        <url-pattern>/classview</url-pattern>
    </servlet-mapping>
 
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>interactive.UploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>   
	
    <servlet>
        <servlet-name>UploadRptServlet</servlet-name>
        <servlet-class>interactive.UploadRptServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadRptServlet</servlet-name>
        <url-pattern>/uploadrpt</url-pattern>
    </servlet-mapping>  	
    
    <servlet>
        <servlet-name>DoTestServlet</servlet-name>
        <servlet-class>interactive.DoTestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DoTestServlet</servlet-name>
        <url-pattern>/dotest</url-pattern>
    </servlet-mapping>  
	
	<error-page>
		<error-code>404</error-code>
		<location>/error.jsp</location>
	</error-page>
	
	<error-page>
		<error-code>500</error-code>
		<location>/error.jsp</location>
	</error-page>
</web-app>